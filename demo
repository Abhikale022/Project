package com.scb.creditcardorigination.customerLoginSignup;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.data.jpa.repository.config.EnableJpaRepositories;
import org.springframework.boot.autoconfigure.domain.EntityScan;

@SpringBootApplication
@EnableJpaRepositories(basePackages = "com.scb.creditcardorigination.customerLoginSignup.Repository")
@EntityScan(basePackages = "com.scb.creditcardorigination.customerLoginSignup.Model")
public class Startc {
    public static void main(String[] args) {
        SpringApplication.run(Startc.class, args);
    }
}