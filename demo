package com.scb.creditcardorigination;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.domain.EntityScan;
import org.springframework.data.jpa.repository.config.EnableJpaRepositories;

@SpringBootApplication
@EnableJpaRepositories(basePackages = "com.scb.creditcardorigination.")
@EntityScan(basePackages = {"com.scb.creditcardorigination","com.scb.creditcardorigination.salesUser.model","com.scb.creditcardorigination.TransactionProcessingSystem.model"})
public class StartApplicationMain {
        public static void main(String[] args) {
            SpringApplication.run(StartApplicationMain.class, args);
        }


    }


